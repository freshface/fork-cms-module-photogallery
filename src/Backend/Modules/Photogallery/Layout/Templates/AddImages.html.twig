

{% extends '/Layout/Templates/base.html.twig' %}

{% block actionbar %}

{% endblock %}


{% block content %}

<form>

    <div class="panel panel-default">
      <div class="panel-heading">
        <p>{{ 'lbl.Images'|trans|ucfirst }}</p>
      </div>
      <div class="panel-body">


                  <div id="js-uploadifive-queue"></div>
                  <p>
                      <input id="js-uploadifive" name="files" type="file" multiple="true">
                  </p>
             

      </div>
    </div>

    <div class="row fork-module-actions">
   <div class="col-md-12">
     <div class="btn-toolbar">

        <div class="btn-group pull-left" role="group">
            <a href="{{ geturl('edit') }}&id={{ record.id }}">
             {{ 'lbl.Cancel'|trans|ucfirst }}
            </a>
        </div>
       <div class="btn-group pull-right" role="group">



         <a href="#"  class="btn btn-primary js-upload-start">
           <span class="fa fa-upload"></span>{{ 'lbl.Upload'|trans|ucfirst }}
         </a>

       </div>
     </div>
   </div>
 </div>

</form>

{% endblock %}
